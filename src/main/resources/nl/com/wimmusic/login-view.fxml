<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER" prefHeight="327.0" prefWidth="654.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints halignment="CENTER" maxWidth="302.0" minWidth="10.0" prefWidth="293.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="549.0" minWidth="10.0" prefWidth="361.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Button fx:id="loginButton" disable="true" mnemonicParsing="false" onAction="#onLoginButtonClick" prefHeight="58.0" prefWidth="106.0" text="Login" GridPane.columnIndex="1" GridPane.rowIndex="3">
            <font>
                <Font size="13.0" />
            </font>
        </Button>
        <Label text="Login">
            <font>
                <Font size="42.0" />
            </font>
        </Label>
        <Label text="Username" GridPane.rowIndex="1">
            <font>
                <Font size="13.0" />
            </font>
        </Label>
        <Label text="Password" GridPane.rowIndex="2">
            <font>
                <Font size="13.0" />
            </font>
        </Label>
        <PasswordField fx:id="passwordField" onTextChange="#onPasswordTextChange" prefHeight="19.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <TextField fx:id="usernameField" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label fx:id="loginFailedLabel" text="Login Failed, Check if the password and username is correct" textFill="#ff0404" visible="false" GridPane.columnIndex="1">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</GridPane>
